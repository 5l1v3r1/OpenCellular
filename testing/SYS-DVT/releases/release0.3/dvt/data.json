{
	"module": {
		"bb": {
			"get": {
				"temperature": {
				                    "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
						            "file":"temp1_input",
						            "cmd":"cat",
					                "unit": "mCelcius"
				},
				"temperature_alert": {
				                    "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
						            "file":"temp1_max",
						            "cmd":"cat",
					                "unit": "mCelcius"
				},

				"temperature_hyst": {
				                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
						           "file":"temp1_max_hyst",
						           "cmd":"cat",
					               "unit": "mCelcius"
				},

                "shunt_voltage": {
                                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"in0_input",
                                   "cmd":"cat",
                                   "unit": "mV"
                },

                "bus_voltage": {
                                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"in1_input",
                                   "cmd":"cat",
                                   "unit": "mV"
                },
                "current": {
                                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"curr1_input",
                                   "cmd":"cat",
                                   "unit": "mA"
                },
                "current_alert": {
                                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"alert_limit_register",
                                   "cmd":"cat",
                                   "unit": "mA"
                },
               "bb_alert_gpio":{
                                  "path":"/sys/class/gpio/",
                                  "file":"gpio1/value",
                                  "pre": "echo 1 > /sys/class/gpio/export",
                                  "cmd":"cat",
                                  "unit":" interrupt_flag"
               },
               "fe_alert_gpio":{
                                "path":"/sys/class/gpio/",
                                "file":"gpio13/value",
                                "pre":"echo 13 > /sys/class/gpio/export",
                                "cmd":"cat",
                                "unit":"interrupt_flag"
                            
                             }
               },
			"set": {
				"temperature_alert": {
				                    "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
						            "file":"temp1_max",
						            "cmd":"echo",
					                "max": 125000,
					                "min": -55000,
                                    "init":125000,
					                "unit": "mCelcius"
				                    },
				"temperature_hyst": {
				                    "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
                                    "file":"temp1_max_hyst",
						            "cmd":"echo",
					                "max": 10000,
					                "min": 0,
                                    "init":0,
					                "unit": "mCelcius"
				},
				"current_alert": {
				                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"alert_limit_register",
						           "cmd":"echo",
					               "max": 2500,
					               "min": 600,
                                   "init":2500,
					               "unit": "mA"
				},
				"bb_alert_enable":{
				        		  "path":"/sys/devices/soc.0/0.dvt/",
				        		  "file":"bb_current_temp_alert_enable",
				        		  "cmd":"echo",
				        		  "init":1
				 },   
                "fe_alert_enable":{
                                    "path":"/sys/devices/soc.0/0.dvt/",
                                    "file":"fe_temp_alert_enable",
				                    "cmd":"echo",
                                    "init":1
                    
                  },
                 "dvt_alert_status":{
                                        
                                      "path":"/sys/devices/soc.0/0.dvt/",
                                      "file":"dvt_alert_status",
                                      "cmd":"echo",
                                      "init":0
   
                        }
                }
		},
		"help": "./occmd.py <module_name> <get/set> <param/all> <value>",
		"version": "0.2",
		"reliability": null
}
}
