{
	"module": {
		"bb": {
			"get": {
				"temperature": {
				                    "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
						            "file":"temp1_input",
						            "cmd":"cat",
					                "unit": "Celcius"
				},
				"temperature_alert": {
				                    "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
						            "file":"temp1_max",
						            "cmd":"cat",
					                "unit": "Celcius"
				},

				"temperature_hyst": {
				                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
						           "file":"temp1_max_hyst",
						           "cmd":"cat",
					               "unit": "Celcius"
				},

                "shunt_voltage": {
                                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"in0_input",
                                   "cmd":"cat",
                                   "unit": "mV"
                },

                "bus_voltage": {
                                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"in1_input",
                                   "cmd":"cat",
                                   "unit": "mV"
                },
                "current": {
                                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"curr1_input",
                                   "cmd":"cat",
                                   "unit": "mA"
                },
                "current_alert": {
                                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"alert_limit_register",
                                   "cmd":"cat",
                                   "unit": "mA"
                }
                
               },
			"set": {
				"temperature_alert": {
				                    "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
						            "file":"temp1_max",
						            "cmd":"echo",
					                "max": 125,
					                "min": -55,
                                    "init":125000,
					                "unit": "Celcius"
				                    },
				"temperature_hyst": {
				                    "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0048/",
                                    "file":"temp1_max_hyst",
						            "cmd":"echo",
					                "max": 10,
					                "min": 0,
                                    "init":0,
					                "unit": "Celcius"
				},
				"current_alert": {
				                   "path":"/sys/devices/soc.0/1180000001000.i2c/i2c-0/0-0044/",
                                   "file":"alert_limit_register",
						           "cmd":"echo",
					               "max": 2500,
					               "min": 600,
                                   "init":2500,
					               "unit": "mA"
				},
				"dvt":         {
				        		  "path":"/sys/devices/soc.0/0.dvt/",
				        		  "file":"bb_current_temp_alert_enable",
				        		  "cmd":"echo",
				        		  "init":1
				               }
				}
				
		},
		"help": "./occmd.py <module_name> <get/set> <param/all> <value>",
		"version": "0.1"
	}
}
